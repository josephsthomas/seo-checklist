# Firebase Configuration
# Get these values from Firebase Console > Project Settings > General
VITE_FIREBASE_API_KEY=your_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id

# App Configuration
VITE_APP_NAME=SEO Checklist Pro
VITE_APP_URL=http://localhost:5173

# Claude AI Configuration (for Image Alt Generator, Meta Generator, Schema Generator)
# Get your API key from https://console.anthropic.com
VITE_CLAUDE_API_KEY=your_claude_api_key_here

# Alternative: Use a proxy server (if you don't want to expose key in browser)
# VITE_AI_PROXY_URL=https://your-proxy-server.com/api

# AI Readability Checker - API Keys (server-side proxy only, NOT VITE_ prefixed)
# These keys are used by the proxy server, never exposed to the browser
# OPENAI_API_KEY=your_openai_api_key_here
# GEMINI_API_KEY=your_gemini_api_key_here
# PERPLEXITY_API_KEY=your_perplexity_api_key_here (Phase 2)
